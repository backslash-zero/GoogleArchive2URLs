{
	"name": "Google Archive Album 2 URL",
	"description": "Getting all the fixed URLs from an Album in Google Archive",
	"version": "1.0",
	"manifest_version": 3,
	"icons": {
		"16": "/images/icons/default_icon16.png",
		"32": "/images/icons/default_icon32.png",
		"48": "/images/icons/default_icon48.png",
		"128": "/images/icons/default_icon128.png"
	},
	"action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "/images/icons/default_icon16.png",
			"32": "/images/icons/default_icon32.png",
			"48": "/images/icons/default_icon48.png",
			"128": "/images/icons/default_icon128.png"
		}
	},
	"permissions" : ["storage", "scripting", "activeTab"],
	"background": {}	
}
